<app xmlns="http://bender.igel.co.jp" xmlns:svg="http://www.w3.org/2000/svg"
  xmlns:xlink="http://www.w3.org/1999/xlink">
  <title>SVG runtime test: set viewbox</title>
  <view>
    <svg:use xlink:href="#r"/>
    <target q="defs" unique="true">
      <svg:rect x="100" y="200" width="300" height="400" fill="#ff4040" id="r"/>
    </target>
    <target q="svg">
      <attribute name="viewBox" value="0 0 600 600"/>
    </target>
  </view>
</app>
